<h5 class="center-align bold">ADMIN</h5>
<br><br>
<table class="table table-striped table-dark">
    <tr>
        <th>Name</th>
        <th>Date of birth</th>
        <th>Gender</th>
        <th>Address</th>
        <th>Email</th>
    </tr>
    {{#each adminList }}
    <tr>
        <td>{{name}}</td>
        <td>{{dob}}</td>
        <td>{{gender}}</td>
        <td>{{address}}</td>
        <td>{{email}}</td>
    </tr>
    {{/each}}
</table>
