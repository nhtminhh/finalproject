<div class="container col s6" style="background-color: #606666;">
<form action="/manager/add" method="post">
   <h5 class="center-align bold">EDIT MANAGER</h5>
   <div class="row">
      <div class="input-field col s12 m4">
         <label>Name</label>
         <input class="validate" type="text" name="name" required id="" autocomplete="off" value="{{manager.name}}">
      </div>
   </div>
   <div class="row">
      <div class="input-field col s12 m4">
         <label>Password</label>
         <input class="validate" type="password" name="password" required id="password" autocomplete="off" value="{{manager.password}}">
      </div>
   </div>
   {{!-- <div class="row">
      <div class="input-field col s12 m4">
         <label>Retype Password</label>
         <span class="retype_error"></span>
         <input class="validate" type="password" name="retype" required id="retype" autocomplete="off" value="{{manager.password}}">
      </div>
   </div> --}}
   <div class="row">
      <div class="input-field col s12 m4">
         <label>Date of birth</label>
         <br><br>
         <span class="retype_error"></span>
         <input class="validate" type="date" name="dob" required id="dob" autocomplete="off" value="{{manager.dob}}">
      </div>
   </div>
   <div class="row">
      <div class="input-field col s12 m4">
         <label>Gender</label>
         <span class="retype_error"></span>
         <input class="validate" type="text" name="gender" required id="gender" autocomplete="off" value="{{manager.gender}}">
      </div>
   </div>
   <div class="row">
      <div class="input-field col s12 m4">
         <label>Address</label>
         <span class="retype_error"></span>
         <input class="validate" type="text" name="address" required id="address" autocomplete="off" value="{{manager.address}}">
      </div>
   </div>
   <div class="row">
      <div class="input-field col s12 m4">
         <label>Email</label>
         <span class="retype_error"></span>
         <input class="validate" type="text" name="email" required id="email" autocomplete="off" value="{{manager.email}}"> 
      </div>
   </div>
   <br>
   <div class="row">
      <div class="col s12 m4 center-align">
         <input class="btn" type="submit" value="Edit">
      </div>
   </div>
</form>
</div>