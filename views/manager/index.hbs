<h1 style="text-align: center;">Manager List</h1>
<a type="button" class="btn btn-info" href="/auth/manager/add">
    <i class="material-icons">add</i>
</a>
<br><br>
<table class="table table-primary">
    <tr>
        <th>Name</th>
        <th>Date of birth</th>
        <th>Gender</th>
        <th>Address</th>
        <th>Email</th>
        <th>Menu</th>
    </tr>
    {{#each managerList }}
    <tr>
        <td>{{name}}</td>
        <td>{{dob}}</td>
        <td>{{gender}}</td>
        <td>{{address}}</td>
        <td>{{email}}</td>
        <td>
            <a type="button" class="btn orange" href="/auth/manager/edit/{{_id}}">
                <i class="material-icons">edit</i>
            </a>
            <a type="button" class="btn red" href="/auth/manager/delete/{{_id}}" onclick="return confirm('Do you want to delete this manager?')">
                <i class="material-icons">delete</i>
            </a>
        </td>
    </tr>
    {{/each}}
</table>
